    "@11ty/eleventy": "^0.11.0",
    "axios": "^0.19.2",
    "countup.js": "^2.0.6",
    "leaflet": "^1.6.0",
    "leaflet-providers": "^1.10.1",
    "vue": "^2.6.11",
    "vue-countup-v2": "^4.0.0",
    "vue-gtag": "^1.10.0",
